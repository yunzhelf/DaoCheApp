<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.yifactory.daocheapp.biz.home_function.home_recommend_tab.activity.HomeRecommendHeadLineActivity">

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#f3f3f3"/>
    <!--<WebView
        android:id="@+id/head_line_wv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></WebView>-->
    <com.yifactory.daocheapp.widget.BaseSwipeRefreshLayout
        android:id="@+id/head_line_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <android.support.v7.widget.RecyclerView
            android:id="@+id/head_line_rv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        </android.support.v7.widget.RecyclerView>
    </com.yifactory.daocheapp.widget.BaseSwipeRefreshLayout>

</com.zhy.autolayout.AutoLinearLayout>
